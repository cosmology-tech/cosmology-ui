{
  "name": "@cosmology-ui/storybook",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "lint": "yarn lint:fix && yarn style",
    "lint:ts": "eslint --ext '**/*.{ts,tsx}'",
    "lint:fix": "eslint --ext '**/*.{ts,tsx}' --fix",
    "storybook": "start-storybook -p 11001",
    "style": "prettier --write '**/*.{ts,tsx}' --config .prettierrc.json",
    "build-storybook": "build-storybook",
    "deploy": "AWS_PROFILE=webinc aws s3 sync storybook-static/ s3://storybook.cosmology.tech",
    "deploy:test": "AWS_PROFILE=webinc aws s3 sync storybook-static/ s3://storybook-test.cosmology.tech",
    "invalidate": "AWS_PROFILE=webinc aws cloudfront create-invalidation --distribution-id E2YEMOBBOGNKLT --paths \"/*\"",
    "invalidate:test": "AWS_PROFILE=webinc aws cloudfront create-invalidation --distribution-id E2SMNXRC02SKJF --paths \"/*\"",
    "deploy:all": "npm run build-storybook && npm run deploy",
    "deploy:all:test": "npm run build-storybook && npm run deploy:test"
  },
  "dependencies": {
    "@cosmology-ui/react": "*",
    "@types/node": "18.14.2",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "eslint": "8.35.0",
    "eslint-config-next": "13.2.1",
    "next": "13.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "7.19.1",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addons": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@types/node": "18.14.5",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "babel-loader": "^9.1.2",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-unused-imports": "^2.0.0",
    "prettier": "2.8.4",
    "sb": "^6.5.16"
  }
}
